{
  "env": {
    "test": {
      "plugins": [
        "istanbul"
      ]
    }
  },
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    "add-module-exports",
    "transform-object-assign",
    ["transform-es2015-modules-umd", {
      "globals": {
        "lodash": "_",
        "jquery": "$"
      },
      "exactGlobals": true
    }]
  ]
}
